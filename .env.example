# Port Configuration
OPENUI_PORT=3000
TIKA_PORT=9998
SEARXNG_PORT=8080
POSTGRES_PORT=5400
MEMORY_SERVICE_PORT=8000

# OpenUI Configuration
OPENUI_SAFE_MODE=false
OPENUI_ENV=prod
OPENUI_GLOBAL_LOG_LEVEL=INFO
OPENUI_WEBUI_URL=http://your-domain.com
OPENUI_CORS_ALLOW_ORIGIN=https://your-domain.com
OPENUI_ENABLE_ADMIN_CHAT_ACCESS=false
OPENUI_ENABLE_ADMIN_EXPORT=false
OPENUI_ENABLE_AUTOCOMPLETE_GENERATION=false
OPENUI_ENABLE_EVALUATION_ARENA_MODELS=false
OPENUI_AIOHTTP_CLIENT_TIMEOUT_MODEL_LIST=3

# Database Configuration (CHANGE THESE CREDENTIALS!)
POSTGRES_USER=openui
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=openui
DATABASE_URL=postgresql://openui:your_secure_password_here@postgres:5432/openui

# Memory Service Configuration
LOG_LEVEL=INFO
MAX_RESULTS_PER_QUERY=10
SIMILARITY_THRESHOLD=0.7

# Build Configuration
BUILDKIT_INLINE_CACHE=1

# Logging Configuration
LOG_MAX_SIZE=10m
LOG_MAX_FILE=3